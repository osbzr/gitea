{{template "base/head" .}}
<div class="explore users">
	{{template "explore/navbar" .}}
	<div class="ui container">
		{{template "explore/search" .}}

		<div class="ui user list">
			{{range .Trans}}
				<div class="item">
				  <div class="content">
				  <!--TODO Show user name-->
				  {{.FromID}} {{.Why}} {{.Qty}} {{.ToID}}
				  </div>
				</div>
			{{else}}
				<div>no result</div>
			{{end}}
		</div>

		{{template "base/paginate" .}}
	</div>
</div>
{{template "base/footer" .}}
